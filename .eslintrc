{
  "parser": "babel-eslint",
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      0,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "max-len": [
      2, 120, 2
    ]
  },
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended"
}